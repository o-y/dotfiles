{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "iterm-fuzzy-history-search",
  "title": "iTerm Fuzzy History Search",
  "description": "Searches .zshhistory and then surfaces that into iTerm",
  "icon": "command-icon.png",
  "author": "o-y",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Shell history search",
      "subtitle": "Searches through the terminal shell history",
      "description": "",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.27.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}